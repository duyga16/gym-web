@model IEnumerable<gym.Models.Feedback>
@{
    @foreach (var feedback in Model)
    {
        <div class="ts_item">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <div class="ti_pic">
                        <img src="@Url.Content("~/img/users/user.jpg")" alt="">
                    </div>
                    <div class="ti_text">
                        <p>
                            @feedback.Details
                        </p>
                        <h5>@feedback.User?.FullName</h5>
                        <div class="tt-rating">
                            @for (int i = 0; i < feedback.Rating; i++)
                            {
                                <i class="fa fa-star"></i>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
}
